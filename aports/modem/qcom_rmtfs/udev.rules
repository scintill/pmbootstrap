# check for mmcblk0 so that an SD card or something doesn't match
KERNEL=="mmcblk0p[0-9]*", ENV{ID_PART_ENTRY_NAME}=="modemst1",    SYMLINK+="disk/qcom_rmtfs/modem_fs1"
KERNEL=="mmcblk0p[0-9]*", ENV{ID_PART_ENTRY_NAME}=="modemst2",    SYMLINK+="disk/qcom_rmtfs/modem_fs2"
KERNEL=="mmcblk0p[0-9]*", ENV{ID_PART_ENTRY_NAME}=="fsg",         SYMLINK+="disk/qcom_rmtfs/modem_fsg"
